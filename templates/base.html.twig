                                    id="path1389"
                                    inkscape:connector-curvature="0"
                                    sodipodi:nodetypes="cssccsscc" />
                        </g>
                        <g
                                inkscape:label="Calque 1"
                                inkscape:groupmode="layer"
                                id="layer1"
                                transform="translate(0.73541382,-241.95204)"
                                style="display:inline">
                            <path
                                    style="fill:#ffffff;fill-opacity:1;stroke:none;stroke-width:0.03036975px;stroke-linecap:butt;stroke-linejoin:miter;stroke-opacity:1"
                                    d="m -0.72556933,249.69873 2.84467843,-10.26471 2.2203392,0.006 1.1950347,9.55718 2.1330124,-9.55844 2.2468241,-0.003 1.6917365,10.46832 -2.2416511,0.68803 -0.9096412,-6.75842 -1.0686659,6.86126 -3.8421003,0.0266 -0.717543,-6.8974 -1.3383482,6.69897 z"
