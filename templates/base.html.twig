                            xmlns:svg="http://www.w3.org/2000/svg"
                            xmlns="http://www.w3.org/2000/svg"
                            width="34"
                            height="34"
                            viewBox="0 0 14.287499 14.2875"
                            version="1.1"
                             >
                        <g
                                inkscape:groupmode="layer"
                                id="layer3"
                                inkscape:label="s"
                                transform="translate(0.73541382,-77.243716)">
