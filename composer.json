{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.2",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.0.*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/expression-language": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/http-client": "5.0.*",
        "symfony/intl": "5.0.*",
        "symfony/mailer": "5.0.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.0.*",
        "symfony/orm-pack": "*",
        "symfony/process": "5.0.*",
        "symfony/security-bundle": "5.0.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/string": "5.0.*",
        "symfony/translation": "5.0.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.0.*",
        "symfony/var-dumper": "5.0.*",
        "symfony/web-link": "5.0.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "deployer/deployer": "^6.8",
        "deployer/recipes": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpmd/phpmd": "^2.9",
        "phpro/grumphp-shim": "^1.3",
        "phpstan/phpstan": "^0.12.76",
        "phpunit/phpunit": "^9.5",
        "sebastian/phpcpd": "^6.0",
        "spatie/phpunit-watcher": "^1.23",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "veewee/composer-run-parallel": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpcsf": "php -d memory_limit=4G vendor/bin/php-cs-fixer fix --dry-run --allow-risky=yes",
        "phpcsff": "php -d memory_limit=4G vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "stan": "php -d memory_limit=4G vendor/bin/phpstan analyse -c phpstan.neon",
        "unit": "php bin/console doctrine:database:drop --if-exists --force --env=test && php bin/console doctrine:database:create --env=test && php bin/console doctrine:schema:update --force --env=test && php -d memory_limit=4G vendor/bin/phpunit",
        "unitw": "php vendor/bin/phpunit-watcher watch",
        "phpmd": "php vendor/bin/phpmd src/ text phpmd-ruleset.xml --exclude *src/Migrations*",
        "phpcpd": "php vendor/bin/phpcpd src/ --exclude *src/Migrations*",
        "lint_twig": "php -d memory_limit=4G bin/console lint:twig templates/",
        "lint_translation": "php -d memory_limit=4G bin/console lint:yaml translations",
        "lint_container": "php -d memory_limit=4G bin/console lint:container",
        "qa": [
            "@parallel phpcsff stan phpmd phpcpd"
        ],
        "symfony_lint": [
            "@parallel lint_twig lint_translation lint_container"
        ],
        "symfony_check": "symfony security:check",
        "test": [
            "@unit"
        ],
        "ci": [
            "@qa",
            "@test"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
